
CREATE TABLE USER(EMAIL VARCHAR(25) primary key, PASSWORD VARCHAR(255), USERNAME VARCHAR(25), GENDER VARCHAR(10));
CREATE TABLE ARTICLE(image VARCHAR(500), publishedBy VARCHAR(255), title VARCHAR(100), content VARCHAR(1024), a_ID VARCHAR(50) primary key, created_at VARCHAR(20));
CREATE TABLE COMMENT(publishedBy varchar(255) REFERENCES USER(EMAIL) ON DELETE CASCADE, comment VARCHAR(500), a_ID varchar(40) REFERENCES ARTICLE(a_ID)); 
CREATE TABLE LIKES(likedBy varchar(25) REFERENCES USER(EMAIL) ON DELETE CASCADE, a_ID varchar(40) REFERENCES ARTICLE(a_ID)); 
CREATE TABLE CONTACT(EMAIL varchar(25), FullName varchar(50), message varchar(255));
